
public class Stringdemo {

	public static void main(String[] args) {
		String s1 = new String("Hello");
		String s2 = new String("Hello");
		String s3 = "Hello";
		String s4 = "Hello";
		if(s1 == s2) {
			System.out.println("True");		
			}
		else {
			System.out.println("False");
		}
		if(s3 == s4) {
			System.out.println("True");		
			}
		else {
			System.out.println("False");
		}
		
	}
}
